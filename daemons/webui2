#!/usr/bin/zsh

webui2-conf() {}

webui2-exec() {
    fn webui2-exec $*
    
    func "launching webui2 on $hostname.$lan"

    if [[ $DEBUG = 2 ]]; then
		hostname=$hostname \
				domain=$lan \
				address=$address \
				interface=$interface \
				launch webui2
    else
		hostname=$hostname \
				domain=$lan \
				address=$address \
				interface=$interface \
				launch webui2 -p $H/run/webui2.pid 1>/dev/null 2>/dev/null &!
    fi
}

